{
    "name": "soliphp/framework",
    "description": "Soli PHP Framework.",
    "type": "library",
    "keywords": ["framework", "soliphp", "soli"],
    "homepage": "https://github.com/soliphp/framework",
    "support": {
        "issues": "https://github.com/soliphp/framework/issues",
        "source": "https://github.com/soliphp/framework"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ueaner",
            "email": "ueaner@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-mbstring": "*",
        "psr/log": "^1.0",
        "psr/container": "^1.0",
        "nikic/fast-route": "^1.2"
    },
    "replace": {
        "soliphp/di": "*",
        "soliphp/events": "*",
        "soliphp/view": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "suggest": {
        "smarty/smarty": "Smarty templating system",
        "twig/twig": "Twig templating system"
    },
    "provide": {
        "psr/container-implementation": "^1.0",
        "psr/event-manager-implementation": "^1.0",
        "psr/log-implementation": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Soli\\": "src/Soli/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soli\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpcs"
        ],
        "phpunit": "./vendor/bin/phpunit",
        "phpcs": "./vendor/bin/phpcs"
    },
    "minimum-stability": "dev"
}
