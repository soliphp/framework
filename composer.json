{
    "name": "soliphp/framework",
    "description": "Soli PHP Framework.",
    "type": "library",
    "keywords": ["framework", "soliphp", "soli"],
    "homepage": "https://github.com/soliphp/framework",
    "support": {
        "issues": "https://github.com/soliphp/framework/issues",
        "source": "https://github.com/soliphp/framework"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ueaner",
            "email": "ueaner@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-mbstring": "*",
        "soliphp/di": "^2.0",
        "soliphp/events": "^1.3"
    },
    "provide": {
        "psr/container-implementation": "^1.0",
        "psr/event-manager-implementation": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Soli\\": "src/Soli/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soli\\Tests\\": "tests/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    },
    "minimum-stability": "dev"
}
